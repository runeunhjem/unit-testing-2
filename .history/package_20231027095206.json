{
  "name": "unit-testing-2",
  "version": "0.0.1",
  "description": "Retake on Unit Testing with Jest",
  "main": "index.js",
  "type": "module",
  "scripts": {
  "test": "npm run test-unit && npm run test-e2e",
  "test-unit": "jest",
  "test-e2e": "npm run cypress-check && npm run test-e2e-cli",
